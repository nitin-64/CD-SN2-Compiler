extern void printI(int32 val2);

void fun(){
	int32 arr[] = {4,3,2,1};
	int32 i;
	int32 j;
	int32 temp;
	i= 0;
    
	while(i<4){
		printI(arr[i]);
		i=i+1;
	}

	for(i=0;i<4;i=i+1){
		for(j=i+1;j<4;j=j+1){
			if(arr[i] > arr[j]){
				temp = arr[i];
				arr[i] = arr[j];
				arr[j] = temp;
			}
		}
	}

	for(i=0;i<4;i=i+1){
		printI(arr[i]);
	}
	return;
}

fun();
